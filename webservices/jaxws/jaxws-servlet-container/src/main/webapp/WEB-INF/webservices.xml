<?xml version="1.0" encoding="UTF-8"?>
<webservices xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ts="http://abhijitsarkar.certification.edu/ocewsd/jaxws/servlet/container/"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/javaee_web_services_1_3.xsd"
	version="1.3">
	<webservice-description>
		<!-- Must be unique in this document -->
		<webservice-description-name>TimeService</webservice-description-name>
		<port-component>
			<!-- @WebService.name -->
			<port-component-name>TimeServiceServletContainer</port-component-name>
			<!-- @WebService.serviceName -->
			<wsdl-service>ts:TimeServiceServletContainer</wsdl-service>
			<!-- @WebService.portName -->
			<wsdl-port>ts:TimeServiceServletContainerPort</wsdl-port>
			<!-- @WebService.endpointInterface -->
			<service-endpoint-interface>edu.certification.abhijitsarkar.ocewsd.jaxws.servlet.container.TimeServiceSEI</service-endpoint-interface>
			<service-impl-bean>
				<!-- Must link to a physical web.xml Servlet definition -->
				<servlet-link>edu.certification.abhijitsarkar.ocewsd.jaxws.servlet.container.TimeService</servlet-link>
			</service-impl-bean>
		</port-component>
	</webservice-description>
</webservices>